<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="669.0" prefWidth="927.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.widok.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TilePane alignment="TOP_CENTER">
      <children>
      
          <Label accessibleRole="TEXT_AREA" alignment="TOP_CENTER" contentDisplay="TOP" prefHeight="22.0" prefWidth="170.0" text="Szyfrowanie AES">
            <font>
               <Font name="System Bold" size="18.0" />
            </font>
         </Label>
      </children>
   </TilePane>
   <Pane prefHeight="590.0" prefWidth="887.0">
      <children>
         <TextField fx:id="key" layoutX="87.0" layoutY="15.0" prefHeight="25.0" prefWidth="792.0" />
         <Label layoutX="14.0" layoutY="10.0" prefHeight="35.0" prefWidth="96.0" text="KLUCZ:">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Label>
         <Label alignment="CENTER" contentDisplay="CENTER" layoutX="87.0" layoutY="40.0" prefHeight="43.0" prefWidth="792.0" text="Długość klucza to: 128 bitów">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Label>
         <Button fx:id="keyGenerator" layoutX="87.0" layoutY="83.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="792.0" onAction="#setKey" text="Wygeneruj klucz">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Button>
         <Label alignment="CENTER" contentDisplay="CENTER" layoutX="6.0" layoutY="124.0" prefHeight="43.0" prefWidth="878.0" text="SZYFROWANIE/DESZYFROWANIE">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Label>
         <Label layoutY="172.0" prefHeight="35.0" prefWidth="215.0" text="Plik z tekstem jawnym:">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Label>
         <Label layoutY="207.0" prefHeight="35.0" prefWidth="215.0" text="Plik z szyfrogramem:">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Label>
         <Button layoutX="215.0" layoutY="177.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="266.0" text="Zapisz">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Button>
         <Button layoutX="215.0" layoutY="211.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="266.0" text="Zapisz">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Button>
         <Button layoutX="534.0" layoutY="211.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="266.0" text="Załaduj">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Button>
         <Button layoutX="534.0" layoutY="176.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="266.0" text="Załaduj">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Button>
         <Label layoutX="15.0" layoutY="256.0" prefHeight="35.0" prefWidth="186.0" text="TEKST JAWNY:">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Label>
         <Label layoutX="529.0" layoutY="256.0" prefHeight="35.0" prefWidth="186.0" text="SZYFROGRAM:">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Label>
         <TextArea fx:id="tekst_jawny" accessibleRole="TEXT_FIELD" layoutX="14.0" layoutY="292.0" prefHeight="276.0" prefWidth="320.0" />
         <TextArea fx:id="tekst_zaszyfrowany" accessibleRole="TEXT_FIELD" layoutX="529.0" layoutY="292.0" prefHeight="276.0" prefWidth="320.0" />
         <Button onAction="#setEncrypt" layoutX="345.0" layoutY="346.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="170.0" text="Szyfruj &gt;&gt;&gt;">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Button>
         <Button onAction="#setDecrypt" layoutX="345.0" layoutY="413.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="170.0" text="&lt;&lt;&lt; Deszyfruj">
            <font>
               <Font name="Txt" size="14.0" />
            </font>
         </Button>
      </children>
   </Pane>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
